syntax = "proto3";

package pose_detector;

import "pose.proto";

message DetectPoseRequest {
  string frame_id = 1;
}

message DetectPoseResponse {
  pose.PoseReading pose = 1;
}

service {
    protocol = "grpc"
    port = 8081
}

function detect-pose {
    request = DetectPoseRequest
    response = DetectPoseResponse
}

function detect-pose2 {
    request = DetectPoseRequest
    response = DetectPoseResponse
}